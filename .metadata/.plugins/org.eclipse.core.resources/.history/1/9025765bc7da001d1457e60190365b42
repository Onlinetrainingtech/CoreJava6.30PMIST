
public class Sample 
{
    void get1()
    {
    	int pid=1001;//local variable
    	System.out.println("Welcome to the get1 function"+pid);
    }
    void get2()
    {
    	System.out.println("Welcome to the get2 function"+pid);
    }
	public static void main(String[] args) {
		
		Sample s1=new Sample();
		s1.get1();
		s1.get2();

	}

}
